{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "cSpell.ignorePaths": ["*.env"],
  "cSpell.words": [
    "apikey",
    "daisyui",
    "hcaptcha",
    "inbucket",
    "messagebird",
    "oklch",
    "oneshot",
    "orioledb",
    "pangea",
    "pooler",
    "prefersdark",
    "SENDGRID",
    "signup",
    "signups",
    "supabase",
    "textlocal",
    "timebox",
    "totp",
    "unlisten",
    "Unprioritized",
    "vite",
    "vonage",
    "workos"
  ],
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.tsdk": "node_modules/typescript/lib"
}
