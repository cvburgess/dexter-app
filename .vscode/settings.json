{
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "always",
    "source.fixAll.eslint": "always"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "cSpell.ignorePaths": [
    "*.env"
  ],
  "cSpell.words": [
    "apikey",
    "daisyui",
    "hcaptcha",
    "inbucket",
    "messagebird",
    "oklch",
    "oneshot",
    "orioledb",
    "pangea",
    "pooler",
    "prefersdark",
    "SENDGRID",
    "signup",
    "signups",
    "supabase",
    "textlocal",
    "timebox",
    "totp",
    "unlisten",
    "Unprioritized",
    "vite",
    "vonage",
    "workos"
  ]
}